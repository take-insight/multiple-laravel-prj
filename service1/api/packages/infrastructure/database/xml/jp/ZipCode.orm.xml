<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="packages\domain\model\zipcode\ZipCode" table="sampleowner.zips">
        <embedded
                name="id"
                class="packages\domain\model\zipcode\ZipCodeId"
                use-column-prefix="false"
        />
        <embedded
                name="zipCode"
                class="packages\domain\model\zipcode\ZipCodePostalCode"
                use-column-prefix="false"
        />
        <field name="prefecture" column="kenmei" type="string">
        </field>
        <field name="city" column="sikumei" type="string">
        </field>
        <field name="townArea" column="sikuika" type="string">
        </field>
        <field name="prefectureCode" column="ken_code" type="integer">
        </field>
        <embedded
                name="userId"
                class="packages\domain\model\zipcode\ZipCodeUserId"
                use-column-prefix="false"
        />
    </entity>
</doctrine-mapping>
